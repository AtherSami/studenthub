<section class="vh-100 gradient-custom">
    <div class="container py-5 h-100">
        <div class="row justify-content-center align-items-center h-100">
            <div class="col-12 col-lg-9 col-xl-7">
                <div class="card shadow-2-strong card-registration" style="border-radius: 15px;">
                    <div class="card-body p-4 p-md-5">




                        <h3 class="mb-4 pb-2 pb-md-0 mb-md-5" align="center">Sign Up</h3>
                        <form asp-action="Signup">

                            
                                   
                            <div class="row">
                                <div class="col-md-6 mb-4">


                                    <div class="form-group">
                                        <label asp-for="Name" class="control-label"></label>
                                        <input asp-for="Name" class="form-control" required />
                                        <span asp-validation-for="Name" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-6 mb-4 pb-2">

                                    <div class="form-group">

                                        <label class="controll-label" asp-for="Password">Password</label>
                                        <input type="password" asp-for="Password" class="form-control form-control" required />
                                        <span asp-validation-for="Password" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>

                            
                           
                           

                            <div class="mt-4 pt-2">
                                <div class="form-group" align="center">
                                        @*<input type="submit" value="Create" class="btn btn-primary" />*@
                                    <button class="css-1nmcspn" value="submit" type="submit"><span class="nessie-text css-jh65zo"> Sign Up</span></button>

                                </div>
                            </div>
                            <span class="nessie-text css-rtpi4b"><span>By signing up, you agree to #'s <a target="_blank" href="//www.classdojo.com/terms">Terms of Service</a> and <a target="_blank" href="//www.classdojo.com/privacy">Privacy Policy</a></span></span>
                            <div class="css-1hxd7po"></div>
                            <h2 class="nessie-text css-fgyd9j" align="center">Already have a parent account?  </h2>
                            <a class="css-1il45xl" asp-action="login"><span class="nessie-text css-jh65zo">Log in here</span></a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
